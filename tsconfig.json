{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "isolatedModules": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./lib",
    "allowSyntheticDefaultImports": true,
    "rootDir": "src/scripts"
  },
  "filesGlob": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "!./node_modules/**/*.ts"
  ],
  "files": [
    "src/scripts/client.ts",
    "src/scripts/fileuploader.ts",
    "src/scripts/index.ts",
    "src/scripts/interface.ts",
    "src/scripts/utilities.ts",
    "src/scripts/models/assets-collection.ts",
    "src/scripts/models/index.ts",
    "src/scripts/ui/assets-preview.ts",
    "src/scripts/ui/filebutton.ts",
    "src/scripts/ui/gallery.ts",
    "src/scripts/ui/index.ts",
    "src/scripts/ui/mime-types.ts",
    "src/scripts/ui/preview-handlers.ts",
    "src/scripts/ui/templates.ts",
    "src/scripts/ui/thumbnailer.ts",
    "src/scripts/ui/utils.ts",
    "src/scripts/ui/cropper/crop-preview.ts",
    "src/scripts/ui/cropper/crop-view.ts",
    "src/scripts/ui/cropper/index.ts",
    "src/scripts/ui/cropper/interfaces.ts",
    "src/scripts/ui/list/index.ts",
    "src/scripts/ui/list/list-item.ts",
    "src/scripts/ui/list/list.ts",
    "src/scripts/ui/preview-handlers/audio-preview.ts",
    "src/scripts/ui/preview-handlers/image-preview.ts",
    "src/scripts/ui/preview-handlers/index.ts",
    "src/scripts/ui/preview-handlers/video-preview.ts",
    "./typings/index.d.ts"
  ]
}